{
	"author": "Jeremiah Lee",
	"description": "The website of Jeremiah Lee",
	"devDependencies": {
		"@11ty/eleventy": "^1.0.1",
		"@11ty/eleventy-plugin-rss": "^1.1.0",
		"@cloudflare/workers-types": "^3.2.0",
		"@percy/cli": "^1.0.0-beta.71",
		"@tailwindcss/typography": "^0.5.1",
		"autoprefixer": "^10.4.2",
		"moment": "^2.29.1",
		"postcss": "^8.4.6",
		"tailwindcss": "^3.0.12"
	},
	"engines": {
		"node": ">= 14.17.0"
	},
	"keywords": [],
	"license": "UNLICENSED",
	"name": "www.jeremiahlee.com",
	"private": true,
	"scripts": {
		"build": "npm run build:html && npm run build:css",
		"build:css": "npx tailwindcss --minify -i css/style.css -o dist/soul-class-style-badass.css --config tailwind.config.js",
		"build:html": "npx @11ty/eleventy --config=eleventy.config.js",
		"dev:css": "npx tailwindcss --watch -i css/style.css -o dist/soul-class-style-badass.css --config tailwind.config.js",
		"dev:serve": "ELEVENTY_ENV=development npx @11ty/eleventy --config=eleventy.config.js --serve",
		"dev:cloudflare": "npx wrangler pages dev ./dist",
		"dev:spotify": "npx tailwindcss build -i site/posts/failed-squad-goals/failed-squad-goals.source.css -o site/posts/failed-squad-goals/failed-squad-goals.compiled.css --config site/posts/failed-squad-goals/tailwind.config.js",
		"build:spotify": "npx tailwindcss build --minify -i site/posts/failed-squad-goals/failed-squad-goals.source.css -o site/posts/failed-squad-goals/failed-squad-goals.compiled.css --config site/posts/failed-squad-goals/tailwind.config.js",
		"dev:burp": "npx tailwindcss build -i site/posts/last-burp-of-hope/last-burp-of-hope.source.css -o site/posts/last-burp-of-hope/last-burp-of-hope.compiled.css --config site/posts/last-burp-of-hope/tailwind.config.js",
		"build:burp": "npx tailwindcss build --minify -i site/posts/last-burp-of-hope/last-burp-of-hope.source.css -o site/posts/last-burp-of-hope/last-burp-of-hope.compiled.css --config site/posts/last-burp-of-hope/tailwind.config.js",
		"dev:raw": "cd site/posts/remote-async-work && NODE_ENV=development postcss remote-async-work.source.css -o remote-async-work.compiled.css -w",
		"build:raw": "npx tailwindcss build --minify -i site/posts/remote-async-work/remote-async-work.source.css -o site/posts/remote-async-work/remote-async-work.compiled.css --config site/posts/remote-async-work/tailwind.config.js",
		"build:erg": "npx tailwindcss build --minify -i site/posts/lgbtq-erg/lgbtq-erg.source.css -o site/posts/lgbtq-erg/lgbtq-erg.compiled.css --config site/posts/lgbtq-erg/tailwind.config.js"
	},
	"version": "5.0.0"
}
