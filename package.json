{
    "author": "Jeremiah Lee",
    "dependencies": {
        "install": "^0.13.0",
        "npm": "^6.14.4"
    },
    "description": "The website of Jeremiah Lee",
    "devDependencies": {
        "@11ty/eleventy": "^0.10.0",
        "@fullhuman/postcss-purgecss": "^2.1.2",
        "autoprefixer": "^9.7.6",
        "cssnano": "^4.1.10",
        "moment": "^2.24.0",
        "postcss-cli": "^7.1.0",
        "tailwindcss": "^1.2.0"
    },
    "keywords": [],
    "license": "UNLICENSED",
    "name": "jeremiahlee.com",
    "private": true,
    "scripts": {
        "build": "npm run build:html && npm run build:css && npm run build:redirects",
        "build:css": "./node_modules/.bin/postcss css/style.css -o dist/soul-class-style-badass.css",
        "build:html": "npx @11ty/eleventy --config=eleventy.config.js",
        "build:redirects": "node generate-redirects.js",
        "dev:css": "npx tailwind build css/style.css -o site/soul-class-style-badass.css",
        "dev:spotify": "npx tailwind build site/posts/failed-squad-goals/failed-squad-goals.source.css -o site/posts/failed-squad-goals/failed-squad-goals.compiled.css",
        "dev:html": "ELEVENTY_ENV=development npx @11ty/eleventy --config=eleventy.config.js --serve"
    },
    "version": "3.0.0"
}
