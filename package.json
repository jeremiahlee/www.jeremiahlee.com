{
	"author": "Jeremiah Lee",
	"description": "The website of Jeremiah Lee",
	"devDependencies": {
		"@11ty/eleventy": "^0.12.1",
		"@11ty/eleventy-plugin-rss": "^1.1.0",
		"@fullhuman/postcss-purgecss": "^4.0.3",
		"@tailwindcss/typography": "^0.4.0",
		"autoprefixer": "^10.2.5",
		"cssnano": "^5.0.8",
		"moment": "^2.29.1",
		"postcss": "^8.2.13",
		"postcss-cli": "^8.3.1",
		"tailwindcss": "^2.2.15",
		"tailwindcss-cli": "^0.1.2"
	},
	"engines": {
		"node": ">= 14.17.0"
	},
	"keywords": [],
	"license": "UNLICENSED",
	"name": "www.jeremiahlee.com",
	"private": true,
	"scripts": {
		"build": "npm run build:html && npm run build:css && npm run build:redirects",
		"build:css": "NODE_ENV=production tailwindcss-cli -i css/style.css -o dist/soul-class-style-badass.css --config tailwind.config.js",
		"build:html": "eleventy --config=eleventy.config.js",
		"build:redirects": "node generate-redirects.js",
		"dev:css": "NODE_ENV=production tailwindcss-cli -i css/style.css -o dist/soul-class-style-badass.css --config tailwind.config.js -w",
		"dev:serve": "ELEVENTY_ENV=development eleventy --config=eleventy.config.js --serve",
		"dev:spotify": "tailwindcss-cli build -i site/posts/failed-squad-goals/failed-squad-goals.source.css -o site/posts/failed-squad-goals/failed-squad-goals.compiled.css --config site/posts/failed-squad-goals/tailwind.config.js",
		"build:spotify": "NODE_ENV=production tailwindcss-cli build -i site/posts/failed-squad-goals/failed-squad-goals.source.css -o site/posts/failed-squad-goals/failed-squad-goals.compiled.css --config site/posts/failed-squad-goals/tailwind.config.js",
		"dev:burp": "tailwindcss-cli build -i site/posts/last-burp-of-hope/last-burp-of-hope.source.css -o site/posts/last-burp-of-hope/last-burp-of-hope.compiled.css --config site/posts/last-burp-of-hope/tailwind.config.js",
		"build:burp": "NODE_ENV=production tailwindcss-cli build -i site/posts/last-burp-of-hope/last-burp-of-hope.source.css -o site/posts/last-burp-of-hope/last-burp-of-hope.compiled.css --config site/posts/last-burp-of-hope/tailwind.config.js",
		"dev:raw": "cd site/posts/remote-async-work && NODE_ENV=development postcss remote-async-work.source.css -o remote-async-work.compiled.css -w",
		"build:raw": "NODE_ENV=production tailwindcss-cli build -i site/posts/remote-async-work/remote-async-work.source.css -o site/posts/remote-async-work/remote-async-work.compiled.css --config site/posts/remote-async-work/tailwind.config.js"
	},
	"version": "4.1.0"
}
